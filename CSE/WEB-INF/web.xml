<!-- web.xml -->
<web-app >
    
    <servlet>
        <servlet-name>DeathPredictionServlet</servlet-name>
        <servlet-class>DeathPredictionServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DeathPredictionServlet</servlet-name>
        <url-pattern>/DeathPredictionServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VisitCounterServlet</servlet-name>
        <servlet-class>VisitCounterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VisitCounterServlet</servlet-name>
        <url-pattern>/visitCounter</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

</web-app>
